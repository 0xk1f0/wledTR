{
  "productName": "wledTR",
  "version": "0.1.1",
  "identifier": "org.wledtr.app",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run check && npm run dev",
    "beforeBuildCommand": "npm run check && npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "wledTR",
        "width": 400,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": false,
        "scope": []
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "appimage",
      "deb",
      "rpm"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "android": {
      "minSdkVersion": 29
    }
  },
  "$schema": "../node_modules/@tauri-apps/cli/schema.json"
}